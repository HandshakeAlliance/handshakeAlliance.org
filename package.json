{
  "name": "handshake-alliance-website",
  "version": "0.1.0",
  "description": "Website of HandshakeAlliance.org",
  "main": "index.js",
  "author": "Jake Geiser <jake@urkellabs.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "gatsby": "^2.24.2",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-sharp": "^2.5.11",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "develop": "gatsby clean && gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby clean && gatsby serve",
    "clean": "gatsby clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HandshakeAlliance/handshakealliance.org"
  },
  "bugs": {
    "url": "https://github.com/HandshakeAlliance/handshakealliance.org/issues"
  },
  "keywords": [
    "handshake",
    "alliance",
    "dns",
    "blockchain",
    "crypto"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:prettier/recommended"
    ]
  },
  "eslintIgnore": [
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "gatsby-ssr.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  }
}
